{
	"manifest_version": 3,
	"name": "Komi/Udmurt Spellcheck",
  "short_name": "KUSPELL",
	"version": "1.0.0",
  "description": "Highlights misspelled words on the active webpage.",
  "icons": {},
  "background": {
    "service_worker": "src/background.js", 
    "persistent": false
  },
  "browser_action": {
    "default_title": "Spell check this page"
  },
  "permissions": ["tabs"],
  "web_accessible_resources": [
    "src/dictionaries/*"
  ]
}